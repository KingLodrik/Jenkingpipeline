    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('delpoy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello World-Testing'
            }
        }
        stage('Releas') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
